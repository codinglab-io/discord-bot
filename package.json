{
  "name": "discord-bot",
  "type": "module",
  "private": true,
  "description": "Discord bot for our server",
  "author": "Codinglab <codinglabio@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node src/main.ts",
    "dev": "tsup-node src/main.ts --watch --on-success \"clear && node --enable-source-maps -r dotenv/config dist/main.js\"",
    "lint": "eslint src/**/*.ts --report-unused-disable-directives",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@keyv/redis": "2.7.0",
    "discord.js": "14.11.0",
    "env-var": "7.3.1",
    "keyv": "4.5.2",
    "param-case": "3.0.4"
  },
  "devDependencies": {
    "@types/node": "20.4.2",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "dotenv": "16.3.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "prettier": "3.0.0",
    "tsup": "7.1.0",
    "type-fest": "3.13.0",
    "typescript": "5.1.6"
  },
  "packageManager": "pnpm@8.6.7",
  "engines": {
    "node": "20.4.0"
  }
}
