{
  "name": "discord-bot",
  "type": "module",
  "private": true,
  "description": "Discord bot for our server",
  "author": "Codinglab <codinglabio@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node src/main.ts",
    "dev": "tsup-node src/main.ts --watch --on-success \"clear && node --enable-source-maps -r dotenv/config dist/main.js\"",
    "lint": "eslint src/**/*.ts --report-unused-disable-directives",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@keyv/redis": "2.5.8",
    "discord-api-types": "0.37.42",
    "discord.js": "14.11.0",
    "env-var": "7.3.1",
    "keyv": "4.5.2",
    "param-case": "3.0.4"
  },
  "devDependencies": {
    "@types/is-url": "1.2.30",
    "@types/node": "20.2.0",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "dotenv": "16.0.3",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.8",
    "tsup": "6.7.0",
    "type-fest": "3.10.0",
    "typescript": "5.0.4"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": "20.2.0"
  }
}
