{
  "name": "discord-bot",
  "type": "module",
  "private": true,
  "description": "Discord bot for our server",
  "author": "Codinglab <codinglabio@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node src/main.ts",
    "dev": "tsup-node src/main.ts --watch --on-success \"clear && node --enable-source-maps -r dotenv/config dist/main.js\"",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier -c src",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@keyv/redis": "2.7.0",
    "cheerio": "1.0.0-rc.12",
    "cron": "2.4.3",
    "discord.js": "14.13.0",
    "env-var": "7.4.1",
    "keyv": "4.5.3",
    "open-graph-scraper": "6.2.2",
    "param-case": "3.0.4"
  },
  "devDependencies": {
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "dotenv": "16.3.1",
    "eslint": "8.49.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "prettier": "3.0.3",
    "tsup": "7.2.0",
    "type-fest": "4.3.1",
    "typescript": "5.2.2",
    "vitest": "0.34.4"
  },
  "packageManager": "pnpm@8.7.4",
  "engines": {
    "node": "20.6.1"
  }
}
