{
  "name": "discord-bot",
  "type": "module",
  "private": true,
  "description": "Discord bot for our server",
  "author": "Codinglab <codinglabio@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node src/main.ts",
    "dev": "tsup-node src/main.ts --watch --on-success \"clear && node --enable-source-maps -r dotenv/config dist/main.js\"",
    "lint": "eslint src/**/*.ts --report-unused-disable-directives",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@keyv/redis": "2.6.1",
    "discord.js": "14.11.0",
    "env-var": "7.3.1",
    "keyv": "4.5.2",
    "param-case": "3.0.4"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "dotenv": "16.3.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.8",
    "tsup": "7.0.0",
    "type-fest": "3.12.0",
    "typescript": "5.1.3"
  },
  "packageManager": "pnpm@8.6.3",
  "engines": {
    "node": "20.3.1"
  }
}
